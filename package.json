{
  "name": "fusion-tables-drive-export",
  "description": "Export Google Fusion Tables to Google Drive",
  "version": "1.0.0",
  "license": "",
  "author": "Ubilabs",
  "engines": {
    "node": ">=8"
  },
  "repository": "https://github.com/ubilabs/fusion-tables-drive-export",
  "main": "src/main.js",
  "private": true,
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "prettier:ci": "prettier --list-different \"src/**/**.ts\"",
    "prettier:fix": "prettier --write \"**/**.ts\"",
    "test": "npm run prettier:ci && tslint -p . -e '**/*.json' && npm run build"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false
  },
  "dependencies": {
    "express": "^4.16.4",
    "googleapis": "^35.0.0",
    "json2csv": "^4.3.0",
    "p-limit": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/json2csv": "^4.2.0",
    "@types/node": "^10.12.8",
    "@types/p-limit": "^2.0.0",
    "nodemon": "^1.18.6",
    "prettier": "^1.15.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
